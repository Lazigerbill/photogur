<h1>Add a picture</h1>
<%=form_tag "/pictures" do%>
	<p>
		<%=label_tag "title"%></br>
		<%=text_field_tag "title"%>
	</p>
	<p>
		<%=label_tag "author"%></br>
		<%=text_field_tag "author"%>
	</p>
	<p>
		<%=label_tag "url"%></br>
		<%=text_field_tag "url"%>
	</p>
	<p>
		<%=submit_tab "Save"%>
	</p>
<%end%>